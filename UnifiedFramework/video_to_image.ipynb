   "source": [
    "import cv2 as cv\n",
    "\n",
    "videofile = \"ExampleVideos/10X_LD_1024_R1.avi\"\n",
    "\n",
    "cap = cv.VideoCapture(videofile)\n",
    "\n",
    "# Read the tenth frame\n",
    "for i in range(10):\n",
    "    ret, frame = cap.read()\n",
    "\n",
    "# Save Frame\n",
    "fn = videofile.split(\"/\")[-1].split(\".\")[0] + \".jpg\"\n",
    "\n",
    "cv.imwrite(\"ExampleImages/\" + fn, frame)"
   ]
